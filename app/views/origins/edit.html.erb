<h1 class="display-4">Edit origin</h1>

<%= form_with scope: :origin, model: @origin, local: true do |form| %>

<p>
	<%= form.label :origin %><br>
	<%= form.text_field :origin %>
</p>

<p>
	<%= form.label :phone %><br>
	<%= form.text_field :phone %>
</p>

<div class="form-group">
<%= form.submit({:class => "btn btn-primary"}) %>
</div>

<% end %>

<%= link_to 'Back', origins_path %>