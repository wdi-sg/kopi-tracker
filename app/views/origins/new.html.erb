<h1>Add an Origin</h2>



<%= form_with scope: :origin, url: origins_path, local: true do |f| %>
    <p>
    <%= f.label :location %> <br/>
    <%= f.text_field :location %>
    </p>
    <p class="error">
    <%= show_errors(@origin, :location) %>
  </p>
    <p>
    <%= f.label :phone %><br/>
    <%= f.text_field :phone %>
    </p>
    <p class="error">
    <%= show_errors(@origin, :phone) %>
  </p>
    <p>
    <%= f.submit %>
    </p>
<% end %>